    border-bottom-right-radius: 4px;
}

.backdrop {
    width: 100%;
    height: 100%;
    align-items: center;
    justify-content: center;
    background-color: rgba(43, 34, 33, 0.69);
}

.card {
    background-color: rgb(71, 58, 57);
    width: 40%;
    align-items: center;
    justify-content: center;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
}

.title {
    font-size: 16px;
    -unity-font-style: bold;
    -unity-text-align: upper-center;
    white-space: normal;
    -unity-font: url('project://database/Assets/Fonts/SuperLegendBoy-4w8Y.ttf?fileID=12800000&guid=ebc9b18855f0d314d9a202b9424edb33&type=3#SuperLegendBoy-4w8Y');
    background-color: rgba(0, 0, 0, 0);
    color: rgb(255, 255, 255);
}
