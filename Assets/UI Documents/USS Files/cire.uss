.default-button {
    height: 45px;
    width: 166px;
    position: relative;
    top: 0;
    left: 0;
    background-color: rgb(170, 141, 122);
    justify-content: center;
    align-items: center;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    -unity-font: url('project://database/Assets/Fonts/SuperLegendBoy-4w8Y.ttf?fileID=12800000&guid=ebc9b18855f0d314d9a202b9424edb33&type=3#SuperLegendBoy-4w8Y');
    -unity-font-definition: url('project://database/Assets/Fonts/SuperLegendBoy-4w8Y%20SDF.asset?fileID=11400000&guid=99e9eadb6b4c09a4bbe69fbaf07dcdcc&type=2#SuperLegendBoy-4w8Y SDF');
    color: rgb(255, 255, 255);
    font-size: 12px;
}

